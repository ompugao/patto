{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "patto",
      "camelcase": "Patto",
      "title": "Patto",
      "scope": "source.patto",
      "file-types": [
        "pn"
      ],
      "injection-regex": "^patto$",
      "class-name": "TreeSitterPatto"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the Patto note format",
    "authors": [
      {
        "name": "Patto contributors"
      }
    ],
    "links": {
      "repository": "https://github.com/ompugao/patto"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
